{
  "name": "ngx-flexmonster",
  "description": "Angular wrapper for Flexmonster Pivot Table and Charts",
  "license": "https://www.flexmonster.com/software-license-agreement/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-flexmonster-showcase",
    "build": "npm run build:lib && npm run build:showcase",
    "build:lib": "ng build ngx-flexmonster",
    "build:showcase": "ng build ngx-flexmonster-showcase",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "package:lib": "npx ng-packagr -p projects/ngx-flexmonster/ng-package.json"
  },
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "flexmonster": "^2.9.116",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular/cli": "~14.0.1",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.0.1",
    "typescript": "4.6.2"
  },
  "keywords": [
    "pivot",
    "html5",
    "grid",
    "table",
    "pivottable",
    "pivotgrid",
    "pivotchart",
    "angular"
  ],
  "author": {
    "name": "Flexmonster",
    "email": "help@flexmonster.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flexmonster/ngx-flexmonster"
  },
  "homepage": "https://flexmonster.com/",
  "bugs": {
    "url": "https://flexmonster.com/technical-support/"
  }
}
